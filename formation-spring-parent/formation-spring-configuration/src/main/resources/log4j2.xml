<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>
<Configuration status="WARN">

	<Appenders>
		<Console name="CONSOLE" target="SYSTEM_OUT" follow="true">
			<PatternLayout>
				<!-- <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} 
					- %msg%n"/> -->
				<Pattern>%d{dd/MM/yy HH:mm:ss} -%F %M %L - %m%n</Pattern>
			</PatternLayout>
		</Console>
	</Appenders>

	<Loggers>
		<Logger
			name="fr.insee.sd44configuration.SD44Configuration"
			additivity="false" level="WARN">
			<AppenderRef ref="CONSOLE" />
		</Logger>

		<Root level="ERROR">
			<AppenderRef ref="CONSOLE" />
		</Root>
	</Loggers>

</Configuration>